}

UserRoleAssignment: {
  shape: class
  +assignment_id: UNIQUEIDENTIFIER
  +user_id: UNIQUEIDENTIFIER
  +role_id: UNIQUEIDENTIFIER
  +assigned_at: DATETIME2
}

